{
  "manifest_version": 3,
  "name": "TikTok Ad Block",
  "version": "1.0.0",
  "description": "Auto-scrolls past sponsored posts on TikTok web.",
   "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
